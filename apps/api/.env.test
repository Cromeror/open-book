# Test Environment Configuration
# This file is used when running tests

NODE_ENV=test

# Test Database Configuration (uses postgres-test container on port 5433)
DATABASE_HOST=localhost
DATABASE_PORT=5433
DATABASE_NAME=openbook_test
DATABASE_USER=postgres
DATABASE_PASSWORD=postgres
DATABASE_SSL=false
DATABASE_POOL_SIZE=5

# JWT Configuration (use test secrets)
JWT_SECRET=test-jwt-secret-key-that-is-at-least-32-characters-long
JWT_ACCESS_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d
